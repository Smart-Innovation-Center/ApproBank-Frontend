AAA,KAAA,OAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,cAAA,QAAA,CAAA,IAAA,IAAA,QAAA,CAAA,IAAA,CAAA,OAAA,EAAA;AACA,YAAA,MAAA,CAAA,eAAA,CAAA;AACA,cAAA,IAAA,EAAA,IADA;AAEA,cAAA,KAAA,EAAA,qCAFA;AAGA,cAAA,KAAA,EAAA;AAHA,aAAA,EAIA,IAJA,CAIA,YAAA;AACA,cAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,gBAAA,IAAA,EAAA;AADA,eAAA;AAGA,aARA;AASA;AACA,SAbA,EAcA,KAdA,CAcA,YAAA;AACA,UAAA,MAAA,CAAA,eAAA,CAAA;AACA,YAAA,IAAA,EAAA,IADA;AAEA,YAAA,KAAA,EAAA,uBAFA;AAGA,YAAA,KAAA,EAAA;AAHA,WAAA;AAKA,SApBA;AAqBA;AACA;AA7BA;AAxBA,CAAA","sourcesContent":["<template>\n  <v-app>\n    <v-main>\n      <v-container class=\"fill-height\" fluid>\n        <v-row align=\"center\" justify=\"center\">\n          <v-col cols=\"12\" sm=\"8\" md=\"4\">\n            <v-card class=\"elevation-12\">\n              <v-toolbar class=\"bg-orange\" dark flat>\n                <v-toolbar-title>S'inscrire</v-toolbar-title>\n                <v-spacer></v-spacer>\n                <v-tooltip bottom>\n                  <template v-slot:activator=\"{ on }\">\n                    <v-btn :to=\"{ name: 'home' }\" icon large v-on=\"on\">\n                      <v-icon>mdi-home</v-icon>\n                    </v-btn>\n                  </template>\n                  <span>Accueil</span>\n                </v-tooltip>\n              </v-toolbar>\n              <v-card-text>\n                <v-form ref=\"registerForm\" :value=\"formValid\">\n                  <v-text-field\n                    label=\"Nom d'Utilisateur\"\n                    name=\"name\"\n                    prepend-icon=\"mdi-account\"\n                    type=\"text\"\n                    v-model=\"newUser.name\"\n                  ></v-text-field>\n\n                  <v-text-field\n                    label=\"Email\"\n                    name=\"email\"\n                    prepend-icon=\"mdi-email\"\n                    type=\"email\"\n                    v-model=\"newUser.email\"\n                  ></v-text-field>\n\n                  <v-text-field\n                    id=\"password\"\n                    label=\"Mot de Passe\"\n                    name=\"password\"\n                    prepend-icon=\"mdi-lock\"\n                    type=\"password\"\n                    v-model=\"newUser.password\"\n                  ></v-text-field>\n                </v-form>\n              </v-card-text>\n              <v-card-actions>\n                <router-link :to=\"{ name: 'login' }\">Se connecter</router-link>\n                <v-spacer></v-spacer>\n                <v-btn\n                  class=\"bg-orange\"\n                  :loading=\"loading\"\n                  :disabled=\"loading\"\n                  @click=\"registerUser(), (loader = 'loading')\"\n                >\n                  Inscription\n                  <template v-slot:loader>\n                    <span class=\"custom-loader\">\n                      <v-progress-circular\n                        class=\"text-orange\"\n                        indeterminate\n                      ></v-progress-circular>\n                    </span>\n                  </template>\n                </v-btn>\n              </v-card-actions>\n            </v-card>\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-main>\n  </v-app>\n</template>\n\n<script>\nimport { mapActions } from \"vuex\";\nexport default {\n  name: \"register\",\n  data() {\n    return {\n      formValid: false,\n      newUser: {\n        email: \"\",\n        name: \"\",\n        password: \"\"\n      },\n      loader: null,\n      loading: false\n    };\n  }, \n  watch: {\n    loader() {\n      const l = this.loader;\n      this[l] = !this[l];\n\n      setTimeout(() => (this[l] = false), 3000);\n\n      this.loader = null;\n    }\n  },\n  methods: {\n    ...mapActions({\n      addNotification: \"application/addNotification\",\n      register: \"user/registerUser\"\n    }),\n    registerUser() {\n      if (this.$refs.registerForm.validate()) {\n        this.register(this.newUser)\n          .then(response => {\n            if (response.data && response.data.success) {\n              this.addNotification({\n                show: true,\n                text1: \"Inscription effectuée avec succès :\",\n                text2: \"Veuillez vous connecter !\"\n              }).then(() => {\n                this.$router.push({\n                  name: \"login\"\n                });\n              });\n            }\n          })\n          .catch(() => {\n            this.addNotification({\n              show: true,\n              text1: \"Inscription échouée :\",\n              text2: \"Veuillez vérifier les informations fournies !\"\n            });\n          });\n      }\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/views/auth"}]}