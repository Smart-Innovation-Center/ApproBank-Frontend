              <v-card-title>\r\n                            <span class=\"headline\">Nouveau Validateur</span>\r\n                          </v-card-title>\r\n\r\n                          <v-card-text>\r\n                                <v-form>\r\n                                  <v-text-field\r\n                  v-if=\"!edit\"\r\n                    label=\"Nom de famille\"\r\n                    name=\"firstname\"\r\n                    prepend-icon=\"mdi-account\"\r\n                    type=\"text\"\r\n                    v-model=\"editedItem.user.firstname\"\r\n                  ></v-text-field>\r\n\r\n                  <v-text-field\r\n                  v-if=\"!edit\"\r\n                    label=\"Prénom(s)\"\r\n                    name=\"lastname\"\r\n                    prepend-icon=\"mdi-account\"\r\n                    type=\"text\"\r\n                    v-model=\"editedItem.user.lastname\"\r\n                  ></v-text-field>\r\n\r\n                  <v-text-field\r\n                  v-if=\"!edit\"\r\n                    label=\"Téléphone\"\r\n                    name=\"phone\"\r\n                    prepend-icon=\"mdi-phone\"\r\n                    type=\"text\"\r\n                    v-model=\"editedItem.user.phone\"\r\n                  ></v-text-field>\r\n\r\n                  <v-text-field\r\n                  v-if=\"!edit\"\r\n                    label=\"Nom d'Utilisateur\"\r\n                    name=\"name\"\r\n                    prepend-icon=\"mdi-account\"\r\n                    type=\"text\"\r\n                    v-model=\"editedItem.user.name\"\r\n                  ></v-text-field>\r\n\r\n                  <v-text-field\r\n                  v-if=\"!edit\"\r\n                    label=\"Email\"\r\n                    name=\"email\"\r\n                    prepend-icon=\"mdi-email\"\r\n                    type=\"email\"\r\n                    v-model=\"editedItem.user.email\"\r\n                  ></v-text-field>\r\n\r\n                  <v-text-field\r\n                  v-if=\"!edit\"\r\n                    id=\"password\"\r\n                    label=\"Mot de Passe\"\r\n                    name=\"password\"\r\n                    prepend-icon=\"mdi-lock\"\r\n                    type=\"password\"\r\n                    v-model=\"editedItem.user.password\"\r\n                  ></v-text-field>\r\n\r\n                </v-form>\r\n                          </v-card-text>\r\n\r\n                          <v-card-actions>\r\n                            <v-spacer></v-spacer>\r\n                            <v-btn\r\n                              class=\"btn btn-link btn-default\"\r\n                              text\r\n                              @click=\"close\"\r\n                            >\r\n                              Annuler\r\n                            </v-btn>\r\n                            <v-btn\r\n                              class=\"btn btn-orange\"\r\n                              text\r\n                              @click=\"save\"\r\n                            >\r\n                              Enregister\r\n                            </v-btn>\r\n                          </v-card-actions>\r\n                        </v-card>\r\n                      </v-dialog>\r\n                      \r\n                    </v-toolbar>\r\n      \r\n                  </template>\r\n                  <template v-slot:item.active=\"{ item }\">\r\n                      \r\n                        {{ item.active ? \"Actif\" : \"Gélé\" }}\r\n                      \r\n                  </template>\r\n                  <template v-slot:item.actions=\"{ item }\">\r\n                    <v-switch \r\n                        v-model=\"item.active\"\r\n                        color=\"orange darken-3\"\r\n                        v-on:change=\"geler(item.id)\"\r\n                      >\r\n                      </v-switch>\r\n                  </template>\r\n                  <template v-slot:no-data>\r\n                      Aucune donnée à afficher\r\n                  </template>\r\n                  </v-data-table>\r\n                </template>\r\n                </v-card>\r\n              </div>\r\n            </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </v-app>\r\n</template>\r\n<script>\r\nimport { mapActions, mapGetters, mapState } from \"vuex\";\r\nexport default {\r\n  name: \"validateursOM\",\r\n  data:() => ({\r\n    search: '',\r\n    //singleSelect: false,\r\n    //selected: [],\r\n    edit: false,\r\n      nombreVal: null,\r\n      dialog: false,\r\n      headers: [\r\n        {\r\n          text: 'Nom Utilisateur',\r\n          align: 'start',\r\n          value: 'user.name',\r\n        },\r\n        { text: 'Nom de famille', value: 'user.firstname' },\r\n        { text: 'Prénom(s)', value: 'user.lastname' },\r\n        { text: 'Email', value: 'user.email' },\r\n        { text: 'Telephone', value: 'user.phone' },\r\n        { text: 'Statut', value: 'active' },\r\n        //{ text: 'Date', value: 'created_at' },\r\n        { text: 'Géler', value: 'actions', sortable: false },\r\n      ],\r\n      editedIndex: -1,\r\n      editedItem: {\r\n        user: {\r\n            name: \"\",\r\n            firstname: \"\",\r\n            lastname: \"\",\r\n            phone: \"\",\r\n            email: \"\",\r\n            password: \"\",\r\n        },\r\n        name: \"\",\r\n        firstname: \"\",\r\n        lastname: \"\",\r\n        phone: \"\",\r\n        email: \"\",\r\n        password: \"\",\r\n        active: true\r\n      },\r\n      defaultItem: {\r\n         user: {\r\n            name: \"\",\r\n            firstname: \"\",\r\n            lastname: \"\",\r\n            phone: \"\",\r\n            email: \"\",\r\n            password: \"\",\r\n        },\r\n        name: \"\",\r\n        firstname: \"\",\r\n        lastname: \"\",\r\n        phone: \"\",\r\n        email: \"\",\r\n        password: \"\",\r\n      },\r\n  }),\r\n  computed: {\r\n    ...mapGetters({\r\n      userInfos: \"user/userInfos\",\r\n      validatorsOM: \"validatorOM/validatorsOM\"\r\n    }),\r\n    ...mapState({\r\n      validatorsOM: state => state.validatorOM.validatorsOM\r\n    }),\r\n  },\r\n  watch: {\r\n      dialog (val) {\r\n        val || this.close()\r\n      },\r\n    },\r\n  methods: {\r\n    ...mapActions({\r\n      registerUser: \"user/registerUser\",\r\n      validatorsOMAd: \"validatorOM/addvalidatorsOM\",\r\n      validatorsOMGel: \"validatorOM/gelValidatorsOM\"\r\n    }),\r\n      geler(id) {\r\n        //console.log(id);\r\n        this.validatorsOMGel(id)\r\n      },\r\n      close () {\r\n        this.dialog = false\r\n        this.$nextTick(() => {\r\n          this.editedItem = Object.assign({}, this.defaultItem)\r\n          this.editedIndex = -1\r\n        })\r\n      },\r\n      save () {\r\n        if (!this.editedIndex > -1) {\r\n        this.editedItem.name = this.editedItem.user.name;\r\n        this.editedItem.firstname = this.editedItem.user.firstname;\r\n        this.editedItem.lastname = this.editedItem.user.lastname;\r\n        this.editedItem.email = this.editedItem.user.email;\r\n        this.editedItem.phone = this.editedItem.user.phone;\r\n        this.editedItem.password = this.editedItem.user.password;\r\n        this.validatorsOMAd(this.editedItem);\r\n        this.validatorsOM.push(this.editedItem);\r\n         \r\n        }\r\n        this.close()\r\n      },\r\n    logout() {\r\n      this.$store.dispatch(\"user/logoutUser\").then(() => {\r\n        this.$router.push({ name: \"login\" });\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.$store.dispatch(\"validatorOM/validatorsOM\");\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/views/pages"}]}