A;AAEA,cAAA,KAAA,EAAA,qCAFA;AAGA,cAAA,KAAA,EAAA;AAHA,aAAA,EAIA,IAJA,CAIA,YAAA;AACA,cAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,gBAAA,IAAA,EAAA;AADA,eAAA;AAGA,aARA;AASA;AACA,SAbA,WAcA,YAAA;AACA,UAAA,MAAA,CAAA,eAAA,CAAA;AACA,YAAA,IAAA,EAAA,IADA;AAEA,YAAA,KAAA,EAAA,uBAFA;AAGA,YAAA,KAAA,EAAA;AAHA,WAAA;AAKA,SApBA;AAqBA;AACA;AA7BA;AAxBA,CAAA","sourcesContent":["<template>\r\n  <v-app>\r\n    <v-main>\r\n      <v-container class=\"fill-height\" fluid>\r\n        <v-row align=\"center\" justify=\"center\">\r\n          <v-col cols=\"12\" sm=\"8\" md=\"4\">\r\n            <v-card class=\"elevation-12\">\r\n              <v-toolbar class=\"bg-orange\" dark flat>\r\n                <v-toolbar-title>S'inscrire</v-toolbar-title>\r\n                <v-spacer></v-spacer>\r\n                <v-tooltip bottom>\r\n                  <template v-slot:activator=\"{ on }\">\r\n                    <v-btn :to=\"{ name: 'home' }\" icon large v-on=\"on\">\r\n                      <v-icon>mdi-home</v-icon>\r\n                    </v-btn>\r\n                  </template>\r\n                  <span>Accueil</span>\r\n                </v-tooltip>\r\n              </v-toolbar>\r\n              <v-card-text>\r\n                <v-form ref=\"registerForm\" :value=\"formValid\">\r\n                  <v-text-field\r\n                    label=\"Nom d'Utilisateur\"\r\n                    name=\"name\"\r\n                    prepend-icon=\"mdi-account\"\r\n                    type=\"text\"\r\n                    v-model=\"newUser.name\"\r\n                  ></v-text-field>\r\n\r\n                  <v-text-field\r\n                    label=\"Email\"\r\n                    name=\"email\"\r\n                    prepend-icon=\"mdi-email\"\r\n                    type=\"email\"\r\n                    v-model=\"newUser.email\"\r\n                  ></v-text-field>\r\n\r\n                  <v-text-field\r\n                    id=\"password\"\r\n                    label=\"Mot de Passe\"\r\n                    name=\"password\"\r\n                    prepend-icon=\"mdi-lock\"\r\n                    type=\"password\"\r\n                    v-model=\"newUser.password\"\r\n                  ></v-text-field>\r\n                </v-form>\r\n              </v-card-text>\r\n              <v-card-actions>\r\n                <router-link :to=\"{ name: 'login' }\">Se connecter</router-link>\r\n                <v-spacer></v-spacer>\r\n                <v-btn\r\n                  class=\"bg-orange\"\r\n                  :loading=\"loading\"\r\n                  :disabled=\"loading\"\r\n                  @click=\"registerUser(), (loader = 'loading')\"\r\n                >\r\n                  Inscription\r\n                  <template v-slot:loader>\r\n                    <span class=\"custom-loader\">\r\n                      <v-progress-circular\r\n                        class=\"text-orange\"\r\n                        indeterminate\r\n                      ></v-progress-circular>\r\n                    </span>\r\n                  </template>\r\n                </v-btn>\r\n              </v-card-actions>\r\n            </v-card>\r\n          </v-col>\r\n        </v-row>\r\n      </v-container>\r\n    </v-main>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions } from \"vuex\";\r\nexport default {\r\n  name: \"register\",\r\n  data() {\r\n    return {\r\n      formValid: false,\r\n      newUser: {\r\n        email: \"\",\r\n        name: \"\",\r\n        password: \"\"\r\n      },\r\n      loader: null,\r\n      loading: false\r\n    };\r\n  }, \r\n  watch: {\r\n    loader() {\r\n      const l = this.loader;\r\n      this[l] = !this[l];\r\n\r\n      setTimeout(() => (this[l] = false), 3000);\r\n\r\n      this.loader = null;\r\n    }\r\n  },\r\n  methods: {\r\n    ...mapActions({\r\n      addNotification: \"application/addNotification\",\r\n      register: \"user/registerUser\"\r\n    }),\r\n    registerUser() {\r\n      if (this.$refs.registerForm.validate()) {\r\n        this.register(this.newUser)\r\n          .then(response => {\r\n            if (response.data && response.data.success) {\r\n              this.addNotification({\r\n                show: true,\r\n                text1: \"Inscription effectuée avec succès :\",\r\n                text2: \"Veuillez vous connecter !\"\r\n              }).then(() => {\r\n                this.$router.push({\r\n                  name: \"login\"\r\n                });\r\n              });\r\n            }\r\n          })\r\n          .catch(() => {\r\n            this.addNotification({\r\n              show: true,\r\n              text1: \"Inscription échouée :\",\r\n              text2: \"Veuillez vérifier les informations fournies !\"\r\n            });\r\n          });\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/auth"}]}