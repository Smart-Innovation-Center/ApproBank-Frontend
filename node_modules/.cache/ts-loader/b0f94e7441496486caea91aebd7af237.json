r\n  return new Promise((resolve, reject) => {\r\n      axios\r\n          .delete(`ribDelete/${payload}`)\r\n          .then(response => {\r\n              resolve(response);\r\n          })\r\n          .catch((error)=>{\r\n              reject(error);\r\n          })\r\n  })\r\n},\r\n\r\n}\r\n\r\n"],"sourceRoot":""}]}